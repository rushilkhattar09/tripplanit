<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="anurag.com.utrip1.Activity.SelectPlanActivity$PlaceholderFragment"
    android:orientation="vertical"
    android:background="@drawable/back62">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:paddingBottom="50dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="90">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/must_see"
                        android:id="@+id/imageView1"
                        android:layout_weight="30" />

                    <TextView
                        android:text="Must See"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textview1"
                        android:layout_weight="70"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:paddingRight="10dp" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar1"
                    android:layout_weight="30"
                    android:layout_marginTop="15dp" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/adventure"
                        android:id="@+id/imageView2"
                        android:layout_weight="30" />

                    <TextView
                        android:text="Adventure"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView2"
                        android:layout_weight="70"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:paddingRight="10dp" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar2"
                    android:layout_weight="30"
                    android:layout_marginTop="15dp" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/art"
                        android:id="@+id/imageView3"
                        android:layout_weight="30" />

                    <TextView
                        android:text="Art"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView3"
                        android:layout_weight="70"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:paddingRight="10dp" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar3"
                    android:layout_weight="30"
                    android:layout_marginTop="15dp" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/enter"
                        android:id="@+id/imageView4"
                        android:layout_weight="30" />

                    <TextView
                        android:text="Entertainment"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView4"
                        android:layout_weight="70"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:paddingRight="20dp" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar4"
                    android:layout_weight="30"
                    android:layout_marginTop="15dp" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/shopping"
                        android:id="@+id/imageView5"
                        android:layout_weight="30" />

                    <TextView
                        android:text="Shopping"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView5"
                        android:layout_weight="70"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:paddingRight="10dp" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar5"
                    android:layout_weight="30"
                    android:layout_marginTop="15dp" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/rest"
                        android:id="@+id/imageView6"
                        android:layout_weight="30" />

                    <TextView
                        android:text="Restaurant"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView6"
                        android:layout_weight="70"
                        android:textStyle="bold"
                        android:textAlignment="center"
                        android:paddingRight="10dp" />
                </LinearLayout>

                <SeekBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seekBar6"
                    android:layout_weight="30"
                    android:layout_marginTop="15dp" />

            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:weightSum="100"
                android:elevation="0dp"
                android:layout_height="60dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="70"
                    android:weightSum="100">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
